<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="RewriterConfig" type="URLRewriter.Config.RewriterConfigSerializerSectionHandler, URLRewriter"/>
  </configSections>
  <RewriterConfig>
    <Rules>
      <RewriterRule>
        <LookFor>~/index.htm</LookFor>
        <SendTo>~/index.aspx</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/index_([0-9]+).htm</LookFor>
        <SendTo>~/index.aspx?page=$1</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/about.htm</LookFor>
        <SendTo>~/about.aspx</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/about_([0-9]+).htm</LookFor>
        <SendTo>~/about.aspx?NavID=$1</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/about_([0-9]+)_([0-9]+).htm</LookFor>
        <SendTo>~/about.aspx?NavID=$1&amp;KindID=$2</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/factory.htm</LookFor>
        <SendTo>~/factory.aspx</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/factory_([0-9]+)_([0-9]+).htm</LookFor>
        <SendTo>~/factory.aspx?NavID=$1&amp;KindID=$2</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/factory_([0-9]+)_([0-9]+)_([0-9]+).htm</LookFor>
        <SendTo>~/factory.aspx?NavID=$1&amp;KindID=$2&amp;page=$3</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/factorydetail_([0-9]+)_([0-9]+)_([0-9]+).htm</LookFor>
        <SendTo>~/factorydetail.aspx?NavID=$1&amp;KindID=$2&amp;InfoID=$3</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/news.htm</LookFor>
        <SendTo>~/news.aspx</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/news_([0-9]+).htm</LookFor>
        <SendTo>~/news.aspx?NavID=$1</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/news_([0-9]+)_([0-9]+).htm</LookFor>
        <SendTo>~/news.aspx?NavID=$1&amp;KindID=$2</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/news_([0-9]+)_([0-9]+)_([0-9]+).htm</LookFor>
        <SendTo>~/news.aspx?NavID=$1&amp;KindID=$2&amp;page=$3</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/newsdetail_([0-9]+)_([0-9]+)_([0-9]+).htm</LookFor>
        <SendTo>~/newsdetail.aspx?NavID=$1&amp;KindID=$2&amp;InfoID=$3</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/rd.htm</LookFor>
        <SendTo>~/rd.aspx</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/rd_([0-9]+).htm</LookFor>
        <SendTo>~/rd.aspx?NavID=$1</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/product.htm</LookFor>
        <SendTo>~/product.aspx</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/product_([0-9]+).htm</LookFor>
        <SendTo>~/product.aspx?NavID=$1</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/product_([0-9]+)_([0-9]+).htm</LookFor>
        <SendTo>~/product.aspx?NavID=$1&amp;page=$2</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/product_([0-9]+)_([S])_([^_]*).htm</LookFor>
        <SendTo>~/product.aspx?NavID=$1&amp;S=$2&amp;keyword=$3</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/product_([0-9]+)_([S])_([^_]*)_([0-9]+).htm</LookFor>
        <SendTo>~/product.aspx?NavID=$1&amp;S=$2&amp;keyword=$3&amp;page=$4</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/productdetail_([0-9]+)_([0-9]+).htm</LookFor>
        <SendTo>~/productdetail.aspx?NavID=$1&amp;productID=$2</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/recruitment.htm</LookFor>
        <SendTo>~/recruitment.aspx</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/recruitment_([0-9]+).htm</LookFor>
        <SendTo>~/recruitment.aspx?NavID=$1</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/faq.htm</LookFor>
        <SendTo>~/faq.aspx</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/faq_([0-9]+).htm</LookFor>
        <SendTo>~/faq.aspx?NavID=$1</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/transportation.htm</LookFor>
        <SendTo>~/transportation.aspx</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/transportation_([0-9]+).htm</LookFor>
        <SendTo>~/transportation.aspx?NavID=$1</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/Contact.htm</LookFor>
        <SendTo>~/Contact.aspx</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/Contact_([0-9]+).htm</LookFor>
        <SendTo>~/Contact.aspx?NavID=$1</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/Contact_([0-9]+)_([0-9]+).htm</LookFor>
        <SendTo>~/Contact.aspx?NavID=$1&amp;KindID=$2</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/Feedback.htm</LookFor>
        <SendTo>~/Feedback.aspx</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/Feedback_([0-9]+).htm</LookFor>
        <SendTo>~/Feedback.aspx?NavID=$1</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/Feedback_([0-9]+)_([0-9]+).htm</LookFor>
        <SendTo>~/Feedback.aspx?NavID=$1&amp;KindID=$2</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/map.htm</LookFor>
        <SendTo>~/map.aspx</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/Payment.htm</LookFor>
        <SendTo>~/Payment.aspx</SendTo>
      </RewriterRule>
    </Rules>
  </RewriterConfig>
  <appSettings>
    <!-- 连接字符串是否加密 -->
    <add key="ConStringEncrypt" value="false"/>
    <!-- 数据库连接字符串,（如果采用加密方式，上面一项要设置为true；加密工具，可在官方下载，
     		如果使用明文这样server=127.0.0.1;database=.....,上面则设置为false。 -->

    <!-- Maticsoft.Accounts权限角色管理模块连接字符串-->
    <add key="ConnectionStringAccounts" value="server=127.0.0.1;database=codematic;uid=sa;pwd=1"/>
    <!--其它模块连接字符串，可以不断增加以便同一个项目支持连接多个数据库。如果没有，可以删除该行-->
    <add key="ConnectionString" value="Provider=Microsoft.Jet.OLEDB.4.0;Data Source=|DataDirectory|\\#aetere-sdfei343x9j34u.mdb"/>
    <!--虚拟目录名称(如果是站点，则为空) -->
    <add key="VirtualPath" value=""/>
    <!--登录页地址 -->
    <add key="LoginPage" value="admin/Login.aspx"/>
    <!--默认菜单是否是展开状态-->
    <add key="MenuExpanded" value="false"/>
    <!--实体对象内容缓村的时间（分钟）-->
    <add key="ModelCache" value="30"/>
  </appSettings>
  <system.web>
    <httpModules>
      <add name="ScriptModule" type="System.Web.Handlers.ScriptModule, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
      <add name="ScriptModule" type="System.Web.Handlers.ScriptModule, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
      <add type="URLRewriter.ModuleRewriter, URLRewriter" name="ModuleRewriter"/>
      <add name="ScriptModule" type="System.Web.Handlers.ScriptModule, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
    </httpModules>
    <httpHandlers>
      <add verb="*" path="*.aspx" type="URLRewriter.RewriterFactoryHandler, URLRewriter" />
      <add verb="*" path="*.htm" type="URLRewriter.RewriterFactoryHandler, URLRewriter" />
      <!--不加此节点不能定向html页面-->
    </httpHandlers>
    <httpRuntime executionTimeout="3600" maxRequestLength="1048576" requestPathInvalidCharacters="" requestValidationMode="2.0"/>
    <compilation debug="true" targetFramework="4.0"></compilation>
    <customErrors mode="Off"/>
    <identity impersonate="true"/>
    <authentication mode="Forms">
      <forms name="forums" path="/" loginUrl="Login.aspx" protection="All" timeout="40">
      </forms>
    </authentication>
    <pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID" validateRequest="false"/>
  </system.web>
</configuration>
